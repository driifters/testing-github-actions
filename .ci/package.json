{
  "name": "@tokyodrift1993/testing-github-actions",
  "version": "0.0.2",
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "release-it": "^14.4.1"
  },
  "auto-changelog": {
    "output": "CHANGELOG.md",
    "template": "keepachangelog",
    "commitLimit": false,
    "unreleased": true,
    "ignoreCommitPattern": "(Release (v)?|Merge (remote-tracking )?branch)",
    "sortCommits": "date"
  }
}
